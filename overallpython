emp = ["balaji", "bytexl", "infosys", "kiwi", "mango"]
newlist = []

for x in emp:
  if "b" in x:
    newlist.append(x)

print(newlist)


emp = ["balaji", "bytexl", "infosys", "kiwi", "mango"]

newlist = [x for x in emp if "b" in x]

print(newlist)


numbers = [1, 2, 3, 4]

dd_numbers = [num * 2 for num in numbers]

print(dd_numbers)


n = 51
res = "Even" if n % 2 == 0 else "Odd"
print(res)

n = -51